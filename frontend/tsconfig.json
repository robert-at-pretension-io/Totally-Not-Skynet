{
  "compilerOptions": {
    "allowJs": true,
    "noImplicitAny": false,
    "lib": [
      "ES2022",
      "dom"
    ],
    "baseUrl": "./src",
    "sourceMap": true,
    "outDir": "./dist",
    "target": "es6",
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "node",
    // "strict": true,
    "esModuleInterop": true,
    "noImplicitThis": false,
    "noImplicitOverride": false,
    // "strictNullChecks": true,
    "strictFunctionTypes": true,
    "typeRoots": [
      "./node_modules/@types",
    ],
  },
  "include": [
    "src/**/*",
    "src/generated"
  ]
}